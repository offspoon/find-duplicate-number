list = [1,2,3,4,5,6,7,8,8]
inf = list.index(list[0])
ins = list.index(list[1])
first = list[inf]
second = list[ins]
checker = 1
while checker < len(list):
    if first == second:
        print('There is a duplicate number')
        break
    else:
        inf = inf + 1
        ins = ins + 1
        first = list[inf]
        second = list[ins]
        checker = checker + 1
